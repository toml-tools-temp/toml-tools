{
  "name": "test-prettier-plugin-toml",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^2.8.8",
    "prettier-plugin-toml": "^0.3.5"
  },
  "scripts": {
    "test": "yarn run format:validate",
    "format:validate": "prettier --list-different \"**/*.@(json|md|toml)\"",
    "format:fix": "prettier --write \"**/*.@(json|md|toml)\""
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
